{
    "name": "mu/posts-module",
    "version": "1.0.0",
    "description": "MUPostsModule generated by ModuleStudio 1.3.1.",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\PostsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=2.0.5 <3.0",
            "class": "MU\\PostsModule\\MUPostsModule",
            "displayname": "Posts",
            "url": "posts",
            "capabilities": {
                "user": {"route": "mupostsmodule_content_index"},
                "admin": {"route": "mupostsmodule_content_adminindex"},
                "categorizable": {
                    "entities": [
                        "MU\\PostsModule\\Entity\\ContentEntity"
                    ]
                }
            },
            "securityschema": {
                "MUPostsModule::": "::",
                "MUPostsModule:ItemListBlock:": "Block title::",
                "MUPostsModule:ItemBlock:": "Block title::",
                "MUPostsModule:ModerationBlock:": "Block title::",
                "MUPostsModule:Content:": "Content ID::",
                "MUPostsModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "5.5.9"
        }
    }
}
