{# Purpose of this template: Display contents within an external context #}
{% for content in items %}
    <h3>{{ content|mupostsmodule_formattedTitle }}</h3>
    <p><a href="{{ path('mupostsmodule_content_display', {'slug': content.slug}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
